{
  "name": "a11ywatch-core",
  "version": "1.0.0",
  "description": "a11ywatch central api",
  "main": "./dist/server.js",
  "scripts": {
    "bootstrap": "bash ./bootstrap.sh",
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register src/server.ts",
    "test": "jest --forceExit --maxWorkers 4",
    "lint": "eslint .",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "NODE_ENV=production node dist/server.js",
    "generate": "graphql-codegen"
  },
  "engines": {
    "node": "v14.x"
  },
  "dependencies": {
    "@a11ywatch/log": "^0.3.5",
    "@a11ywatch/website-source-builder": "^0.0.11",
    "apollo-server": "2.25.3",
    "apollo-server-express": "2.25.3",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.0",
    "cron": "1.8.2",
    "date-fns": "2.28.0",
    "dotenv": "8.2.0",
    "exceljs": "^4.2.1",
    "express": "4.17.1",
    "get-page-speed": "^1.0.1",
    "graphql": "14.6.0",
    "graphql-scalars": "^1.13.6",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "0.11.0",
    "jsonwebtoken": "8.5.1",
    "mongodb": "3.7.3",
    "node-fetch": "2.6.0",
    "node-iframe": "^1.5.10",
    "nodemailer": "6.7.2",
    "redis": "^4.0.3",
    "stripe": "7.14.0",
    "subscriptions-transport-ws": "0.3.1",
    "universal-analytics": "^0.4.23",
    "valid-url": "1.0.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@shelf/jest-mongodb": "^1.2.3",
    "@swc/core": "^1.2.117",
    "@swc/jest": "^0.2.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^10.17.51",
    "@types/universal-analytics": "^0.4.5",
    "apollo-server-testing": "^2.21.1",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.4.1",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "4.2.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "keywords": [
    "accessibility",
    "testing",
    "WCAG"
  ],
  "cacheDirectories": [
    "./node_modules/.cache/mongodb-memory-server/mongodb-binaries"
  ]
}
